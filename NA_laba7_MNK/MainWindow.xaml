<Window x:Class="NA_laba7_MNK.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NA_laba7_MNK"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1200">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBox Width="500" x:Name="tbRes"/>
            <StackPanel>
                <Button x:Name="btnMNK" Click="BtnMNK_Click" Width="30" Margin="0,10,0,40" Height="30" Content="btn"/>
                <Canvas x:Name="cnv" Width="609" Height="300"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
